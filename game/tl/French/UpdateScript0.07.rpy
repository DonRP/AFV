    RT "{i}Il n'y a qu'une poignée de personnes qui savent.{/i}"

# game/UpdateScript0.07.rpy:18013
translate french will_meeting_a081ffa6:

    # RT "{i}Who has it out for Mom and I?{/i}"
    RT "{i}Qui en a pour maman et moi ?{/i}"

# game/UpdateScript0.07.rpy:18014
